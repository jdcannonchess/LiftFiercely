<?xml version="1.0" encoding="utf-8"?>
<!--
   Sample data extraction rules for Android 12+ backup.
   This file allows all data to be backed up by default.
-->
<data-extraction-rules>
    <cloud-backup>
        <include domain="sharedpref" path="."/>
        <include domain="database" path="."/>
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="."/>
        <include domain="database" path="."/>
    </device-transfer>
</data-extraction-rules>

